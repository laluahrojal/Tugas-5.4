# List mahasiswa beserta semester mereka
mahasiswa = ["rojal", "tansa", "afida", "ahada"]
semester = [3, 5, 7, 1]

# Menambah mahasiswa baru dan semester
mahasiswa.append("dimas")
semester.append(2)

# Mengubah nilai dalam list
mahasiswa[1] = "tana"
semester[1] = 6

# Mengakses elemen dan menampilkan semua data
print("Data Mahasiswa:")
for i in range(len(mahasiswa)):
    print(f"{mahasiswa[i]} berada di semester {semester[i]}")
